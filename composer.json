{
	"name": "geyzo/phpoo",
	"description": "Aplicação de estudo de Orientação a Objeto e Twig com PHP",
	"type": "project",
	"require": {
		"twig/twig": "^3.22",
		"symfony/var-dumper": "^8.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/",
			"App\\Classes\\": "src/classes/",
			"App\\Controllers\\": "src/controllers/",
			"App\\Functions\\": "src/functions/",
			"App\\Interfaces\\": "src/interfaces/",
			"App\\Models\\": "src/models/",
			"App\\Repositories\\": "src/repositories/",
			"App\\Views\\": "src/views/"
		}
	},
	"authors": [
		{
			"name": "Geyzon Santos do Amaral",
			"email": "geyzon@gmail.com"
		}
